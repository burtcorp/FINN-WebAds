package {  import flash.display.MovieClip;  import flash.display.BitmapData;  import flash.display.Bitmap;  import flash.display.Loader;  import fl.transitions.Tween;  import fl.transitions.easing.*;  import flash.events.Event;  import flash.net.URLRequest;  import flash.text.TextField;  import flash.text.TextFormat;  import flash.text.AntiAliasType;  import flash.external.ExternalInterface;  public class BannerData extends MovieClip {    private var field: TextField = new TextField();    private var format: TextFormat = new TextFormat();	    public function BannerData() {      if (ExternalInterface.available) {        ExternalInterface.call("banner.plugin", "getAdData", '{"customer": 1, "area": 1}', "sendDataBack");        ExternalInterface.addCallback("sendDataBack", ResultHandler);      }      format.size  = 10;	  format.font  = "Monaco";      field.width  = 960;      field.height = 130;	  field.x = 10;      field.y = 120;	  field.antiAliasType = AntiAliasType.ADVANCED;      field.defaultTextFormat = format;      field.text = "Laster...";      addChild(field);	    }    public function ResultHandler( res:Object ):void { 	  if (res.error){		  field.text = "En feil har oppstått: " + res.message; 	  } else {		  var imageLoader:Loader = new Loader();		  imageLoader.contentLoaderInfo.addEventListener(Event.COMPLETE, function(event:Event){			  var image = addChild(imageLoader); 			  var ratio = (130 / image.height);			  image.scaleY = ratio;			  image.scaleX = ratio;			  image.y = 10;			  image.x = stage.width - image.width;			  var t:Tween = new Tween(image, "alpha", Strong.easeIn, 0, 1, 0.5, true);			  field.text = "Lastet "+res.list[0].mainImage + " ferdig.";		  });		  var image = new URLRequest(res.list[0].mainImage);		  imageLoader.load(image);	  }    }		public function getBannerImage(){		}		public function getBannerTitle(){		}		public function resizeContainer(){		}  }}